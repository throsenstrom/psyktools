library(devtools)
d_old <- matrix(c(9, 7, 11, 12, 10,
8, 9, 13, 11, 19,
6, 6, 8, 16, 14,
8, 6, 6, 11, 5,
10, 6, 14, 9, 10,
4, 11, 11, 23, 11,
6, 6, 13, 12, 14,
5, 3, 13, 10, 15,
7, 8, 10, 19, 11,
7, 7, 11, 11, 11), nrow = 10, ncol = 5, byrow = TRUE)
d_young <- matrix(c(8, 10, 14, 20, 21,
6, 7, 11, 16, 19,
4, 8, 18, 16, 17,
6, 10, 14, 15, 15,
7, 4, 13, 18, 22,
6, 7, 22, 16, 16,
5, 10, 17, 20, 22,
7, 6, 16, 22, 22,
9, 7, 12, 14, 18,
7, 7, 11, 19, 21), nrow = 10, ncol = 5, byrow = TRUE)
# Oikeasti jokainen luku on havainto, joskin hivenen eri koetilanteesta.
# Haluttu dimensio olisi 100 X 3, missä 2. sarake on koetilanne ja 3. ikäryhmä.
labs <- c("Counting", "Rhyming", "Adjective", "Imagery", "Intentional")
eysenck_2way <- data.frame(id = 1:100, # henkilön id-luku
obs = vector("numeric", 100), # observation
lab = vector("character", 100), # group label
age = vector("character", 100)) # age group
# Sijoitetaan aineisto luomaamme tyhjään tauluun for-luupilla ja indeksöinneillä
for (i in 1:ncol(d_old)) {
jid <- seq((i-1)*nrow(d_old)+1, i*nrow(d_old)) # juokseva id (kokeile)
eysenck_2way$id[jid] <- jid # i:n sarakkeen id riveille (i-1)*10+1 - i*10
eysenck_2way$obs[jid] <- d_old[,i] # koko sarakkeen havainnot ko riveille
eysenck_2way$lab[jid] <- labs[i] # kondition nimi relevanteille riveille
eysenck_2way$age[jid] <- "old" # kaikki ovat vanhoja tässä taulussa
}
head(eysenck_2way)
for (i in 1:ncol(d_young)) {
jid <- seq((i-1)*nrow(d_young)+1, i*nrow(d_young)) # juokseva id (kokeile)
jid <- jid + prod(dim(d_old)) # sama asia kuin jid + ncol(d_old)*nrow(d_col)
eysenck_2way$id[jid] <- jid # i:n sarakkeen id riveille (i-1)*10+1 - i*10
eysenck_2way$obs[jid] <- d_young[,i] # koko sarakkeen havainnot ko riveille
eysenck_2way$lab[jid] <- labs[i] # kondition nimi relevanteille riveille
eysenck_2way$age[jid] <- "young" # kaikki ovat nuoria
}
tail(eysenck_2way) # nyt nuorten aineisto on taulun lopussa, kuten piti
eysenck_2way$lab <- factor(eysenck_2way$lab)
eysenck_2way$age <- factor(eysenck_2way$age)
str(eysenck_2way)
use_data(eysenck_2way)
